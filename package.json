{
  "name": "graphlib",
  "version": "0.0.1",
  "description": "A Graph object for JavaScript.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist specs",
    "dist": "npm run clean && babel src --out-dir dist && babel tests --out-dir specs",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m 'Updating docs' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'Updating docs' && git push git@github.com:medialab/graphlib gh-pages --force",
    "docs:serve": "npm run docs:prepare && gitbook serve",
    "lint": "eslint src tests test.js",
    "postpublish": "npm run clean",
    "prepublish": "npm run dist",
    "test": "mocha -u exports --compilers js:babel-core/register ./test.js"
  },
  "files": [
    "dist",
    "specs"
  ],
  "keywords": [
    "graph"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medialab/graphlib.git"
  },
  "contributors": [
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medialab/graphlib/issues"
  },
  "homepage": "https://github.com/medialab/graphlib#readme",
  "dependencies": {
    "events": "^1.1.1"
  },
  "devDependencies": {
    "@yomguithereal/eslint-config": "^3.0.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-preset-es2015": "^6.13.2",
    "eslint": "^3.3.1",
    "gitbook-cli": "^2.3.0",
    "gitbook-plugin-addcssjs": "^1.0.2",
    "gitbook-plugin-github": "^2.0.0",
    "gitbook-plugin-prism": "^1.0.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-es2015-destructuring",
        {
          "loose": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "@yomguithereal/eslint-config/es6"
    ],
    "rules": {
      "object-shorthand": 0,
      "quote-props": 0
    }
  }
}
